{
	"name"        : "davidgraham/strangebrew",
	"type"        : "wordpress-theme",
	"description" : "Strangebrew is a modern theme built using the Hybrid Core 5 framework.",
	"keywords"    : ["wordpress"],
	"homepage"    : "http://www.strangebrewdesign.com",
	"license"     : "GPL-2.0-or-later",
	"authors"     : [
		{
			"name"    : "David Graham",
			"email"   : "david.graham@strangebrewdesign.com",
			"homepage": "http://www.strangebrewdesign.com"
		}
	],
	"autoload"    : {
		"psr-4" : {
			"Strangebrew\\" : "app/"
		}
	},
	"require": {
		"php" : ">=5.6",
		"composer/installers" : "~1.0",
		"justintadlock/hybrid-core" : "^5.0.0",
		"justintadlock/hybrid-font": "^1.0",
		"justintadlock/hybrid-breadcrumbs": "^1.0"
	},
	"require-dev": {
		"wptrt/wpthemereview": "*",
		"dealerdirect/phpcodesniffer-composer-installer": "0.4.4"
	}
}
